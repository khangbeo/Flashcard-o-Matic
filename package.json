{
    "name": "project-flashcards",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=20.13.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@testing-library/user-event": "^14.5.2",
        "bootstrap-icons": "^1.11.3",
        "cross-fetch": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-loading": "^2.0.3",
        "react-router-dom": "^6.22.1",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "prebuild": "env CI=true react-scripts test --colors",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "start": "concurrently \"npm run start:server\" \"npm run start:react\"",
        "start:react": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
        "start:server": "json-server ./data/db.json",
        "test": "react-scripts test"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "json-server": "^0.17.4",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
    }
}
